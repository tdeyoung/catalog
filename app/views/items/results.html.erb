
<% if notice %>
  <p id="notice"><%= notice %></p>
<% end %>

<h1>Search Results</h1>

<%= will_paginate %>
<% if @items.present? %>
  <div id="items">
    <%= render @items %>
    <p><%= link_to "Search Again?",  search_path, class: "btn btn-danger"  %></p>
  </div>
<% else %>
  <p>Sorry, there are no music titles containing the term(s) <%= params[:search] %>.</p>
  <p><%= link_to "Search Again?",  search_path, class: "btn btn-danger"  %></p>
<% end %>
<%= will_paginate %>

<div id="item-form" style="display:none;"></div>

<br>






